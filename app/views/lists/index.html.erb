<h1>All lists</h1>

<% @lists.each do |list| %>
  <%= link_to list_path(list), style: "text-decoration: none" do %>
    <div class="card-category" style="background-image: linear-gradient(rgba(0,0,0,0.3), rgba(0,0,0,0.3)), url('<%= cl_image_path list.photo.key, height: 300, width: 400, crop: :fill %>')">
      <%= list.name %>
    </div>
  <% end  %>
<% end %>
<%= link_to "create a new List", new_list_path, class: "btn btn-primary m-5" %>
